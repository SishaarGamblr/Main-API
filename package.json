{
  "name": "main-api",
  "version": "0.0.1",
  "description": "Main API",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "nodemon src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "docker:dev": "docker-compose up",
    "docker:connect": "docker-compose exec app /bin/bash",
    "db:connect": "docker-compose exec postgres psql -d betting_scraper_db -U superuser",
    "db:reset": "docker-compose exec app npx prisma migrate reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SishaarGamblr/Main-API.git"
  },
  "author": "SishaarRao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SishaarGamblr/Main-API/issues"
  },
  "homepage": "https://github.com/SishaarGamblr/Main-API#readme",
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@types/config": "^3.3.0",
    "config": "^3.3.8",
    "fastify": "^4.10.2",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "prisma": "^4.7.1",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "latest",
    "fastify-tsconfig": "latest",
    "nodemon": "latest",
    "prettier": "latest",
    "rimraf": "latest",
    "ts-node": "latest",
    "tsconfig-paths": "latest",
    "typescript": "latest"
  }
}
